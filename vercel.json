{
  "version": 2,
  "builds": [
    {
      "src": "client/project/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "server/api/predict.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/predict",
      "dest": "/server/api/predict.py"
    },
    {
      "src": "/(.*)",
      "dest": "/client/project/$1"
    }
  ]
}
